<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.hzg.measurement.CalibrationSet" table="calibration_sets">
		<id name="id" column="id">
			<generator class="native">
				<param name="sequence">calibration_sets_id_seq</param>
			</generator>
		</id>

		<many-to-one name="observedPropertyInstance" class="de.hzg.measurement.ObservedPropertyInstance" update="false" insert="false">
			<column name="observed_property_instance_id" not-null="true" />
		</many-to-one>

		<property name="parameter1" column="parameter1" />
		<property name="parameter2" column="parameter2" />
		<property name="parameter3" column="parameter3" />
		<property name="parameter4" column="parameter4" />
		<property name="parameter5" column="parameter5" />
		<property name="parameter6" column="parameter6" />

		<property name="validStart">
			<column name="valid_start" not-null="true" />
		</property>

		<property name="validEnd">
			<column name="valid_end" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
