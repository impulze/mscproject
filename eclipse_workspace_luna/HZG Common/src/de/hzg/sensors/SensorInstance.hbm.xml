<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.hzg.sensors.SensorInstance" table="sensor_instances">
		<id name="id" column="id">
			<generator class="native" />
		</id>

		<property name="address">
			<column name="address" not-null="true" />
		</property>

		<property name="calibration1" column="calibration1" not-null="true" />
		<property name="calibration2" column="calibration2" not-null="true" />
		<property name="calibration3" column="calibration3" not-null="true" />
		<property name="calibration4" column="calibration4" not-null="true" />
		<property name="calibration5" column="calibration5" not-null="true" />
		<property name="calibration6" column="calibration6" not-null="true" />

		<many-to-one name="description" class="de.hzg.sensors.SensorDescription">
			<column name="sensor_description_id" />
		</many-to-one>
	</class>
</hibernate-mapping>