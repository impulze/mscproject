<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.hzg.sensors.SensorDescription" table="sensor_descriptions">
		<id name="id" column="id">
			<generator class="native">
				<param name="sequence">sensor_descriptions_id_seq</param>
			</generator>
		</id>

		<property name="name">
			<column name="name" length="255" not-null="true" />
		</property>

		<property name="className">
			<column name="className" length="255" not-null="true" />
		</property>
	</class>
</hibernate-mapping>