<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.hzg.values.RawData" table="raws">
		<id name="id" column="id">
			<generator class="native">
				<param name="sequence">raws_id_seq</param>
			</generator>
		</id>

		<property name="date">
			<column name="date" not-null="true" />
		</property>

		<property name="value">
			<column name="value" />
		</property>

		<property name="average">
			<column name="avg" />
		</property>

		<property name="min">
			<column name="min" />
		</property>

		<property name="max">
			<column name="max" />
		</property>

		<property name="median">
			<column name="median" />
		</property>

		<property name="stddev">
			<column name="stddev" />
		</property>

		<many-to-one name="observedPropertyInstance" class="de.hzg.measurement.ObservedPropertyInstance">
			<column name="observed_property_instance_id" not-null="true" />
		</many-to-one>
	</class>
</hibernate-mapping>