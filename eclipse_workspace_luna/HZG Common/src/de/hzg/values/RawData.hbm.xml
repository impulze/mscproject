<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.hzg.values.RawData" table="raws">
		<id name="id" column="id">
			<generator class="native">
				<param name="sequence">raws_id_seq</param>
			</generator>
		</id>

		<property name="value">
			<column name="value" not-null="true" />
		</property>

		<property name="date">
			<column name="timestamp" not-null="true" />
		</property>

		<many-to-one name="sensorInstance" class="de.hzg.sensors.SensorInstance">
			<column name="sensor_instance_id" />
		</many-to-one>
	</class>
</hibernate-mapping>